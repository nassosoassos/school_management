<% form_for(@san_semester, :url => {:action=> 'subscribe_to_semester'}, :html=> {:multipart=>true, :method=>:post}) do |s| %>
	<%= s.text_field :year, :style => "visibility:hidden", :value => @semester_year %>
	<%= s.text_field :number, :style => "visibility:hidden", :value => @semester_number %>

	<%= submit_tag "", :value => "#{t('subscribe')}", :class => "submit_button" %>
  <% end %>

	
